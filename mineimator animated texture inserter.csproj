<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>mineimator_animated_texture_inserter</RootNamespace>
    <Authors>Melonsboy</Authors>
    <Product>Mineimator Animated Texture inserter</Product>
    <Description>This program inserts images (a lot of them) in your mineimator project to animate them</Description>
